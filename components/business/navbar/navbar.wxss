@import '../../../iconfont.wxss'

.navbar {
  width: 100%;
  overflow: hidden;
  top: 0;
  left: 0;
  z-index: 10;
  position: fixed;
  transition: all 0.3s ease;
}


.navbar-title {
  width: 100%;
  height: 92rpx;
  line-height: 92rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  color: #22292C;
  font-size: 34rpx;
}

.return-back {
  position: absolute;
  left: 30rpx;
  color: #22292C;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
  align-items: center;
  height: 100%;
}
.formIdBox  {
  display: block;
  height: 100%;
}
.line {
  width:2rpx;
  height:36rpx;
  border-left:2rpx solid rgba(151,151,151,1);
  margin:0 22rpx;
  vertical-align:middle;
  display:inline-block;

}
.backLast {
  font-size: 34rpx;
}
.backHome {
  font-size: 39rpx;
}
.formIdBtn {
  padding: 0;
  background: none;
  display:inline-block;
  line-height: 92rpx;
}
.formIdBtn:after {
  display: none;
}

.page-title {
  max-width: 350rpx;
  display: inline-block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-weight: 700;
  font-weight: 34rpx;
}
