/* page/applicant/pages/createUser/createUser.wxss */
.createUser {
	width: 100%;
	height: 100vh;
	background: #4F286E url('https://attach.lieduoduo.ziwork.com/front-assets/images/pic_bg.jpg') no-repeat bottom center;
	background-size: 100% 100%;
}

.start {
	width: 100%;
	height: 100%;
	position: relative;
}
.content {
	position: absolute;
	top: 50%;
	left: 50%;
	transform:translate3d(-100vh, 50%, 0);
}


.start .title {
	width: 504rpx;
	margin: 0 auto;
	display: block;
}
.start .icon {
	width: 222rpx;
	margin: 42rpx auto 0;
	display: block;
}