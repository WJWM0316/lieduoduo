/* page/applicant/pages/center/resumeEditor/aimsEdit/aimsEdit.wxss */
@import '../../../../../../iconfont.wxss';
.warp{
  box-sizing: border-box;
  width: 100%;
  padding: 0 40rpx;
}
.commont{
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 122rpx;
  font-size: 28rpx;
  color: #353943;
  border-bottom: 1px solid #E8E9EB;
}

.commont text{
  color: red;
  line-height: 122rpx;
  margin-right: 10rpx;
  
}
.title {
  min-width: 150rpx;
}
.result {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.result .resultBox {
  width: 80%;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.saveBox {
  width: 100%;
  box-sizing: border-box;
  padding: 29rpx 85rpx;
  display: flex;
  justify-content: space-between;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
}
.save {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-weight:400;
  color:#fff;
  display: block;
  background: #652791;
  text-align: center;
  border-radius: 48rpx;
  font-size: 32rpx;
}
.saveAgain {
  box-sizing: border-box;
  width: 308rpx;
  display: block;
}
.delete{
  width: 248rpx;
  background-color: #BCBCBC;
}