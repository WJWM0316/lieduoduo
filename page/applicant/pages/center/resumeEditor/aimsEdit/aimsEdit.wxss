@import '../../../../../../iconfont.wxss';
.warp{
  box-sizing: border-box;
  width: 100%;
  padding: 0 40rpx;
}
.commont{
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 122rpx;
  font-size: 28rpx;
  color: #353943;
  border-bottom: 1px solid #E8E9EB;
}
.commont .result{
  display: flex;
  align-items: center;
  justify-content: center;
}
.commont text{
  color: red;
  line-height: 122rpx;
  margin-right: 10rpx;
}
.save{
  width: 550rpx;
  height: 88rpx;
  background-color: #652791;
  border-radius:48rpx;
  margin: 0 auto;
  margin-top: 70rpx;
  color: #fff;
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  font-weight: 400;
}
