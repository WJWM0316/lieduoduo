<!--简历项目编辑页-->
<navbar title="项目经历"></navbar>
<view class="warp">
  <view class="commont">
    <view class="title"><text>*</text>项目名称</view>
    <view class="result"><input type="text" bindblur="itemName" placeholder="请输入项目名称" value="{{itemName}}"></input></view>
  </view>
  <view class="commont">
    <view class="title"><text>*</text>担任角色</view>
    <view class="result"><input type="text" bindblur="role" placeholder="请输入" value="{{role}}"></input></view>
  </view>
  <view class="commont salary">
    <view class="title"><text>*</text>开始时间</view>
    <myPicker class="myPicker" setResult="{{info.startTimeDesc}}" pickerType="startTime" placeholderStyle="color: #BCBCBC" data-time="start" bindresultevent="getresult"></myPicker>
  </view>
  <view class="commont salary">
    <view class="title"><text>*</text>结束时间</view>
    <myPicker class="myPicker" setResult="{{info.endTimeDesc}}" pickerType="endTime" placeholderStyle="color: #BCBCBC" data-time="end" bindresultevent="getresult"></myPicker>
  </view>
  <!--项目描述-->
  <view class="title"><text>*</text>项目描述</view>
  <view class="textArea">
    <textarea placeholder="填写项目内容" placeholder-class="placeholder" maxlength="50" bindinput="WriteContent" value="{{description}}"></textarea>
  </view>
  <!--项目链接-->
  <view class="title">项目链接</view>
  <view class="textArea">
    <textarea placeholder="填写项目链接" placeholder-class="placeholder" maxlength="50" bindinput="itemLink" value="{{itemLink}}"></textarea>
  </view>
</view>
<view class="save add" bindtap="add" wx:if="{{isAdd}}">保存</view>
<view class="btn" wx:else>
  <view class="save delete" bindtap="del">删除</view>
  <view class="save" bindtap="save">保存</view>
</view>
