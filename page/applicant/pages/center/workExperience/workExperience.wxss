/* page/applicant/pages/center/secondStep/secondStep.wxss */
.header{
  box-sizing: border-box;
  padding-top: 185rpx;
  position: relative;
  width: 100%;
  height: 482rpx;
}
.headerBg{
  width: 100%;
  position: absolute;
  z-index: -1;
  top: 0;
  left: 0;
}
.header .title{
  text-align: center;
  display: block;
  font-size:56rpx;
  font-weight:600;
  color: #FFFFFF;
}
.header .subtitle{
  text-align: center;
  display: block;
  margin-top: 17rpx;
  font-size:26rpx;
  font-weight:400;
  color: #FFF;
}
.commont{
  border-radius: 50rpx;
  height: 88rpx;
  width: 550rpx;
  background-color: #E8E9EB;
  text-align: center;
  margin: 0 auto;
  margin-top: 56rpx;
  font-size: 32rpx;
}
.commont input{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
}
