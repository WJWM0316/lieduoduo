.ul {
	margin: 122rpx 30rpx 24rpx 30rpx;
	overflow: hidden;
	position: relative;
}

.ul .li {
	background:rgba(255,255,255,1);
	box-shadow:0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
	border-radius:10rpx;
	display: flex;
	align-items: center;
	position: relative;
	padding: 30rpx 40rpx;
}

.ul .li .img-box{
	width:120rpx;
	height:120rpx;
	border:2rpx solid rgba(232,233,235,1);
	border-radius: 50%;
	overflow: hidden;
}

.ul .li .img-box image {
	width: 100%;
	height: 100%;
}

.ul .li .content{
	flex: 1;
	margin-left: 30rpx;
	position: relative;
}

.ul .li .name{
	font-size:36rpx;
	font-weight:500;
	color:rgba(40,40,40,1);
	display: inline-block;
	vertical-align: middle;
}

.ul .li .mix{
	font-size:26rpx;
	font-weight:300;
	color:rgba(146,146,146,1);
	display: inline-block;
	vertical-align: middle;
	margin-left: 30rpx;
}

.ul .li .row-one{
	max-width:calc(100vw - 290rpx);
	line-height:36rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.ul .li .row-two{
	max-width:calc(100vw - 290rpx);
	font-size:28rpx;
	font-weight:300;
	color:rgba(98,98,98,1);
	line-height:1.1;
	margin-top: 20rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.ul .content {
	width: calc(100vw - 60rpx)
}

.ul .button-box {
  height: 180rpx;
  margin-left: 8rpx;
}

.ul .button-box .button {
  width:134rpx;
  height:100%;
  background:rgba(237,92,92,1);
  box-shadow:0rpx 8rpx 20rpx 0rpx rgba(34,56,101,0.08);
  border-radius:10rpx;
  line-height: 180rpx;
  text-align: center;
  font-size:28rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  display: inline-block;
}

.ul .item {
	margin-bottom: 24rpx;
	position: relative;
	width: 100%;
}

.office-status-bar{
  box-sizing: border-box;
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  background-color: #FFFFFF;
  box-shadow:0rpx 11rpx 30rpx 0rpx rgba(153,193,214,0.08);
  position: fixed;
  left: 0;
  z-index: 1;
  display: flex;
}

.office-status-bar::after{
  content: '.';
  display: block;
  opacity: 0;
}

.office-status-bar .item {
  box-sizing: border-box;
  padding: 0 30rpx;
  text-align: center;
  background-color: #FFFFFF;
  margin-left: 10rpx;
  border-radius: 27rpx;
  position: relative;
  font-size:32rpx;
  font-weight:400;
  color:rgba(98,98,98,1);
  display: inline-block;
  flex: 1;
}

.office-status-bar .item::after {
  content: '';
  display: block;
  width:32rpx;
  height:8rpx;
  background:#652791;
  border-radius:4rpx;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  opacity: 0;
  visibility: hidden;
}

.office-status-bar .active {
  font-size:32rpx;
  font-weight:600;
  color:rgba(40,120,255,1);
  font-size:32rpx;
  font-weight:600;
  color:rgba(40,40,40,1);
  pointer-events: none;
}

.office-status-bar .active::after {
  opacity: 1;
  visibility: visible;
}

.office-status-bar::after{
  content: '.';
  display: block;
  opacity: 0;
}

