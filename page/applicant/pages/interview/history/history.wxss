.ul {
  margin: 52rpx 30rpx;
}

.ul .li {
  background:rgba(255,255,255,1);
  box-shadow:0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
  border-radius:10rpx;
  position: relative;
  padding-bottom: 5rpx;
}

.ul .li .row-one {
  display: flex;
  align-items: center;
  padding: 22rpx 0 54rpx 0;
  position: relative;
  margin: 0 28rpx 0 28rpx;
}

.ul .li .img-box {
  width:120rpx;
  height:120rpx;
  box-shadow:0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
  border-radius:50%;
  position: absolute;
  top: -20rpx;
  left: 0;
  border: 4rpx solid white;
  box-sizing: border-box;
  overflow: hidden;
}

.ul .li .img-box image{
  width: 100%;
  height: 100%;
}

.ul .li .content {
  margin-left: 144rpx;
  flex: 1;
}

.ul .li .header {
  max-width:500rpx;
  height:36rpx;
  line-height:36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  position: relative;
}

.ul .li .header .name {
  font-size:36rpx;
  font-weight:600;
  color:rgba(40,40,40,1);
  display: inline;
}

.ul .li .degree:before {
  background: #282828;
  position: absolute;
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  display: block;
  content: '';
  left: 10rpx;
  top: 50%;
  transform: translateY(-50%);
}

.ul .li .header .degree{
  font-size:28rpx;
  font-weight:400;
  color:rgba(40,40,40,1);
  position: relative;
  display: inline;
  padding-left: 30rpx;
  top: -2px;
}

.ul .li .infos1 {
  display: inline-block;
  max-width:340rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ul .li .infos2 {
  max-width:490rpx;
  height:26rpx;
  font-size:26rpx;
  font-weight:400;
  color:rgba(98,98,98,1);
  line-height:26rpx;
  margin-top: 14rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ul .li .time {
  font-size:24rpx;
  font-weight:400;
  color:#BCBCBC;
  display: inline-block;
  float: right;
}
.ul .li .time.color {
  color: #652791;
}
.ul .li .icon {
  color: #BACED8;
  margin-right: 16rpx;
  font-size: 32rpx;
}

.ul .li .row-two {
  max-width:calc(100% - 56rpx);
  line-height: 1;
  font-size:26rpx;
  font-weight:400;
  color:#626262;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 28rpx;
  padding-bottom: 25rpx;
}

.ul .li .row-three {
  max-width:calc(100% - 56rpx);
  line-height: 1;
  font-size:26rpx;
  font-weight:400;
  color:#626262;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 28rpx;
  padding-bottom: 30rpx;
}

.formIdBtn {
  padding: 0;
  background: none;
  text-align: left;
  line-height: normal;
  overflow: unset;
  margin-bottom: 44rpx;
}

.down-show {
  height:98rpx;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 18rpx 20rpx 0rpx rgba(101,39,145,0.04);
  padding: 0 40rpx;
  font-size:28rpx;
  font-weight:400;
  color:rgba(92,86,93,1);
  display: flex;
  align-items: center;
  position: relative;
  z-index: 2;
  position: fixed;
  left: 0;
  right: 0;
}

.down-show-active {
  color: #652791;
}

.down-show .key {
  position: relative;
  display: inline-block;
  line-height: 1;
}

.down-show .iconfont{
  transform: rotate(0deg);
  position: absolute;
  right: -35rpx;
  top: 50%;
  width: 32rpx;
  height: 32rpx;
  margin-top: -16rpx;
  transition: all;
}

.down-show-active .iconfont{
  transform: rotate(180deg);
}

.child-box {
  height:446rpx;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 11rpx 30rpx 0rpx rgba(0,0,0,0.04);
  border-radius:0rpx 0rpx 10rpx 10rpx;
  padding: 0 40rpx;
}

.child-box .p1{
  height:26rpx;
  font-size:26rpx;
  font-weight:300;
  color:rgba(92,86,93,1);
  line-height:26rpx;
  padding-top: 64rpx;
  padding-bottom: 56rpx;
}

.child-box .h2{
  height:26rpx;
  font-size:26rpx;
  font-weight:500;
  color:rgba(40,40,40,1);
  line-height:26rpx;
}

.child-box .box4{
  padding-top: 26rpx;
}

.child-box .box4 .item{
  display: inline-block;
  width:138rpx;
  height:58rpx;
  border-radius:29rpx;
  border:1rpx solid rgba(205,203,207,1);
  margin-right: 18rpx;
  text-align: center;
  line-height: 58rpx;
  font-size:26rpx;
  color:#626262;
}

.child-box .box4 .active{
  color:rgba(101,39,145,1);
  border-color: rgba(101,39,145,1);
}

.child-box .h3{
  height:26rpx;
  font-size:26rpx;
  font-weight:500;
  color:rgba(40,40,40,1);
  line-height:26rpx;
  padding-top: 50rpx;
}

.child-box .select-range{
  height:40rpx;
  font-size:28rpx;
  font-weight:500;
  line-height:40rpx;
  margin: 26rpx 0;
}

.child-box .select-range .start{
  display: inline-block;
  margin-right: 20rpx;
  color:#929292;
  position: relative;
}

.child-box .select-range .picker{
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  opacity: 0;
}

.child-box .select-range .end{
  display: inline-block;
  margin-right: 20rpx;
  color:#929292;
  position: relative;
}

.child-box .select-range .separator{
  display: inline-block;
  margin-right: 20rpx;
  color: #5C565D;
}

.child-box .select-range .icon{
  width: 32rpx;
  height: 32rpx;
}

.child-box .select-range .active {
  color:rgba(101,39,145,1);
}