.item3 {
	background:rgba(255,255,255,1);
	box-shadow:0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
	border-radius:10rpx;
	margin: 30rpx;
	margin-top: 100px;
	position: relative;
}

.item3 .row-one {
	display: flex;
	padding: 40rpx 0;
	border-bottom: 1px dotted #BACED8;
	margin: 0 28rpx;
	align-items: center;
}

.item3 .row-one .img-box {
	width:120rpx;
	height:120rpx;
	border-radius: 50%;
	background: red;
	overflow: hidden;
}

.item3 .row-one image {
	width: 100%;
	height: 100%;
}

.item3 .row-one .content {
	flex: 1;
	margin-left: 24rpx;
}

.item3 .row-one .name {
	width:314rpx;
	height:36rpx;
	font-size:36rpx;
	font-weight:600;
	color:rgba(40,40,40,1);
	line-height:36rpx;
	margin-bottom: 14rpx;
}

.item3 .row-one .mix {
	width:calc(100% - 24rpx - 120rpx - 28rpx - 20rpx);
	height:26rpx;
	font-size:26rpx;
	font-weight:400;
	color:rgba(146,146,146,1);
	line-height:26rpx;
	margin-bottom: 14rpx;
}

.item3 .row-one .degress {
	max-width:490rpx;
	font-size:26rpx;
	font-weight:400;
	color:rgba(98,98,98,1);
	line-height:26rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.item3 .row-two {
  max-width:calc(100% - 56rpx);
  line-height: 1;
  font-size:26rpx;
  font-weight:400;
  color:rgba(98,98,98,1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 28rpx;
}

.item3 .row-two text{
  color: #652791;
}

.item3 .icon-mianshizhiwei {
  color: #DCDCDC;
  margin-right: 16rpx;
  font-size: 32rpx;
  display: inline-block;
}

.item3 .icon-lishijilu {
  margin-right: 5rpx;
}

.item3 .time {
	height:52rpx;
	line-height: 52rpx;
	text-align: center;
	border-radius:26rpx;
	position: absolute;
	top: 20rpx;
	right: 24rpx;
	font-size:32rpx;
	font-weight:bold;
	line-height:52rpx;
	padding: 0 20rpx;
}

.item3 .time.active {
	color:#652791;
	background: #FFDC29;
}

.item3 .time.gray {
	color:#fff;
	background: #E8E9EB;
}

.formIdBtn {
  padding: 0;
  background: none;
  text-align: left;
  line-height: normal;
}

.item2 {
	height:246rpx;
	background:rgba(255,255,255,1);
	box-shadow:0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
	border-radius:10rpx;
}

.item2 .row-one {
	display: flex;
	padding: 40rpx 0;
	border-bottom: 1px dotted #BACED8;
	margin: 0 28rpx;
	align-items: center;
}