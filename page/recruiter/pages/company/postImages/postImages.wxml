<view class="company-images">
	<navbar title="编辑公司图片" background="#fff" color="#22292C"></navbar>
	<view class="ul">
    <image
    	class="commont img"
    	wx:for="{{imgList}}"
    	wx:key="{{item}}"
    	src="{{item.smallUrl}}">
    		<view class="close" data-imgIndex="{{index}}" bindtap="delImg">
    			<i class="icon iconfont icon-close"></i>
    		</view>
    </image>
    <unloadFile class="commont addImg" bindresultEvent="upload" unloadType="img">
      <view class="add"><i class="icon iconfont icon-jiahao"></i></view>
    </unloadFile> 
  </view>
</view>