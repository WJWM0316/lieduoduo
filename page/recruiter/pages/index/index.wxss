.index-bannar image {
  width: 100%;
  height: 430rpx;
  vertical-align: text-top;
  border-top: 20rpx solid #652791;
  display: block;
}

.index-list-box {
  transform: translateY(-130rpx);
}

.default {
  padding: 0 30rpx;
}
.fixed {
  position: fixed;
  left: 0;
  right: 0;
  padding-top: 24rpx;
  z-index: 22;
}
.ul-tab-bar {
  display: flex;
  line-height: 80rpx;
  border-bottom: 1rpx solid #BACED8;
}

.ul-tab-bar .li {
  flex: 1;
  font-size:28rpx;
  font-weight:400;
  color:rgba(188,188,188,1);
  text-align: center;
  position: relative;
  background:rgba(243,245,248,1);
}

.ul-tab-bar .li:first-child {
  border-top-left-radius: 10rpx;
}

.ul-tab-bar .li:last-child {
  border-top-right-radius: 10rpx;
}

.ul-tab-bar .li view {
  width:3rpx;
  height:31rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
  background: rgba(232,233,235,1)
}

.ul-tab-bar .li:before {
  content: '';
  display: block;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -28rpx;
  width: 0;
  height: 0;
  border-width:16rpx;
  border-style: solid;
  border-color: #F7EBFF transparent transparent transparent;
  display: block;
  content: '';
  opacity: 0;
  visibility: hidden;
  z-index: 2;
}

.ul-tab-bar .li:after {
  content: '';
  display: block;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -30rpx;
  width: 0;
  height: 0;
  border-width:16rpx;
  border-style: solid;
  border-color: #BACED8 transparent transparent transparent;
  display: block;
  content: '';
  opacity: 0;
  visibility: hidden;
  z-index: 1;
}

.ul-tab-bar .active {
  background: #F7EBFF;
  pointer-events: none;
  font-size:28rpx;
  font-weight:500;
  color:rgba(101,39,145,1);
}

.ul-tab-bar .active view{
  display: none;
}

.ul-tab-bar .active:nth-child(1) view{
  display: none;
}

.ul-tab-bar .active:before {
  opacity: 1;
  visibility: visible;
}

.ul-tab-bar .active:after {
  opacity: 1;
  visibility: visible;
}

.list-box {
  padding: 0 30rpx;
}

.nodata {
  background:rgba(255,255,255,1);
