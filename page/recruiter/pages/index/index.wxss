}

.default {
  height: 100rpx;
  padding: 0 30rpx;
  border-radius: 16rpx 16rpx 0 0;
  overflow: hidden;
}

.fixed {
  position: fixed;
  left: 0;
  right: 0;
  background: #FBFBFF;
  z-index: 22;
  padding-top: 20rpx;
}

.fixed:before {
  position: absolute;
  left: -30rpx;
  height: 120rpx;
  top: -20rpx;
  content: '';
  display: block;
  background: #FBFBFF;
  right: -30rpx;
}

.ul-tab-bar {
  position: relative;
  height: 100rpx;
  overflow: hidden;
  border-radius: 16rpx 16rpx 0 0;
  z-index: 22;
}

.ul-tab-bar:after {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 5rpx;
  height: 15rpx;
  background: white;
  /*background: red;*/
  content: '';
  display: block;
  z-index: -1;
}

.ul-tab-bar .li {
  font-size:28rpx;
  color:rgba(188,188,188,1);
  text-align: center;
  position: relative;
  background:white;
  line-height: 80rpx;
  width: 50%;
  float: left;
  display: inline-block;
  border-bottom: 1rpx solid rgba(186,206,216,1);
}

.ul-tab-bar .li:before {
  content: '';
  display: block;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -28rpx;
  width: 0;
  height: 0;
  border-width:16rpx;
  border-style: solid;
  border-color: #F7EBFF transparent transparent transparent;
  display: block;
  content: '';
  opacity: 0;
  visibility: hidden;
  z-index: 2;
}

.ul-tab-bar .li:after {
  content: '';
  display: block;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -29rpx;
  width: 0;
  height: 0;
  border-width:16rpx;
  border-style: solid;
  border-color: #BACED8 transparent transparent transparent;
  display: block;
  content: '';
  opacity: 0;
  visibility: hidden;
  z-index: 1;
}
.ul-tab-bar .line {
  background: white;
  position: absolute;
  left: 0;
  right: 0;
  height: 5rpx;
  bottom: 0rpx;
  z-index: 3;
  /*background: red*/
}
.ul-tab-bar .active {
  background: #F7EBFF;
  pointer-events: none;
  font-size:28rpx;
  font-weight:700;
  color:rgba(101,39,145,1);
}
.ul-tab-bar .active .line {
  background: #F7EBFF;
  /*background: red*/
}
.ul-tab-bar .total {
  width:36rpx;
  height:36rpx;
  background:rgba(255,127,76,1);
  border-radius: 50%;
  display: inline-block;
  vertical-align: middle;
  margin-left: 16rpx;
  line-height: 36rpx;
  text-align: center;
  font-size:24rpx;
  font-weight:700;
  color:rgba(255,255,255,1);
}

.ul-tab-bar .active:before {
  opacity: 1;
  visibility: visible;
}

.ul-tab-bar .active:after {
  opacity: 1;
  visibility: visible;
}

