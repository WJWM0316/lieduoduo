<view class="recruiter-index">
	<view class="index-bannar">
		<image src="../../../../images/bg_homepageh.png"></image>
	</view>
	<view class="index-list-box">
		<view class="ul-tab-bar">
			<view class="li {{pageList === 'seen-me' ? 'active' : ''}}" bindtap="changeCompanyLists" data-page-list="seen-me">看过我的</view>
			<view class="li {{pageList === 'interested-me' ? 'active' : ''}}" bindtap="changeCompanyLists" data-page-list="interested-me">对我感兴趣</view>
			<view class="li {{pageList === 'my-loved' ? 'active' : ''}}" bindtap="changeCompanyLists" data-page-list="my-loved">我感兴趣的</view>
		</view>
		<commonList list="{{browseMySelfLists}}" identity="{{identity}}" wx:if="{{pageList === 'seen-me'}}"></commonList>
		<commonList list="{{collectMyList}}" identity="{{identity}}" wx:if="{{pageList === 'interested-me'}}"></commonList>
		<commonList list="{{mapyCollectUser}}" identity="{{identity}}" wx:if="{{pageList === 'my-loved'}}"></commonList>
	</view>
  <tabBar tabType="1"></tabBar>
</view>
