}

.item2 .row-two .iconfont {
  font-size: 30rpx;
  margin-right: 20rpx;
  color: #CDCBCF;
}

.item2 .row-two .right {
  display: inline;
  float: right;
  font-size:26rpx;
	font-weight:300;
	color:rgba(146,146,146,1);
}

.item2 .row-three {
  line-height:28rpx;
  padding-bottom: 40rpx;
  max-width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item2 .row-three .status {
  float: right;
  color: #626262;
}

.item2 .row-three .left {
  display: inline;
  float: left;
	font-size:26rpx;
	font-weight:300;
	color:rgba(40,40,40,1);
}

.item2 .row-three .iconfont {
  font-size: 30rpx;
  margin-right: 20rpx;
  color: #CDCBCF;
}

.item2 .row-three .right {
  display: inline;
  float: right;
  font-size:26rpx;
	font-weight:300;
	color:rgba(146,146,146,1);
}
.item2 .reddot {
  width:14rpx;
  height:14rpx;
  background:#FF4949;
  position: absolute;
  top: 24rpx;
  right: 24rpx;
  border-radius: 50%;
}

.formIdBtn {
  padding: 0;
  background: none;
  text-align: left;
  line-height: normal;
  overflow: unset;
  margin: 20rpx 30rpx;
}
.formIdBtn::after {
  display: none;
}

.jump{
  width:430rpx;
  height:98rpx;
  border-radius:49rpx;
  font-size:32rpx;
  line-height: 98rpx;
  text-align: center;
  margin-top: 24rpx;
  background:#FFDC29;
  box-shadow:0rpx 4rpx 20rpx 0rpx rgba(232,201,40,0.47);
  color:#282828;
}

.rules_boxs{
  font-size:30rpx;
  font-weight:300;
  color:rgba(40,40,40,1);
  line-height:48rpx;
  padding: 0 60rpx 100rpx 60rpx;
}

.position_list, .type_list {
  max-height: 1000rpx;
}

.position_list .list_item {
  background:rgba(255,255,255,1);
  position: relative;
  padding: 40rpx 0;
}

.position_list .list_item::before {
  height: 1px;
  background: #EDEDED;
  content: '';
  display: block;
  position: absolute;
  left: 30rpx;
  right: 0;
  bottom: 0;
}

.position_list .list_item_first {
  height:32rpx;
  font-size:28rpx;
  color:rgba(40,40,40,1);
  line-height:32rpx;
  padding-left: 30rpx;
}

.position_list .title {
  font-weight:700;
  max-width: 480rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
  vertical-align: middle;
}

.position_list .saraly {
  display: inline-block;
  vertical-align: middle;
}

.position_list .list_item_second {
  height:34rpx;
  font-size:24rpx;
  color:rgba(146,146,146,1);
  line-height:34rpx;
  padding-left: 30rpx;
}

.position_list .list_item_second {
  height:34rpx;
  font-size:24rpx;
  color:rgba(146,146,146,1);
  line-height:34rpx;
  margin-top: 14rpx;
}

.position_list .experience,
.position_list .address,
.position_list .degress {
  display: inline-block;
  margin-right: 16rpx;
  padding: 0 12rpx;
  background: #F8F8F8;
  border-radius:4rpx;
}

.position_list .myRadio {
  width:36rpx;
  height:36rpx;
  position: relative;
  display: inline-block;
  border-radius: 50%;
  border: 1px solid #BCBCBC;
  line-height: 36rpx;
  text-align: center;
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  right: 40rpx;
  transform: translateY(-50%);
}

.position_list .myRadio.active {
  background: #652791;
  color: white;
  border: unset;
}

.position_list .myRadio.active .icon{
  font-size: 24rpx;
  display:inline-block;
  transform:translateY(-4rpx);
}

.type_list .type_header{
  line-height:1.2;
  margin-bottom: 30rpx;
}

.type_list .key{
  font-size:37rpx;
  font-weight:700;
  color:rgba(53,64,72,1);
  display: inline-block;
  vertical-align: middle;
}

.type_list .key_fix{
  font-size:28rpx;
  font-weight:300;
  color:rgba(146,146,146,1);
  display: inline-block;
  vertical-align: middle;
}

.type_list .base_ul {
  overflow: hidden;
}

.type_list .city_ul{
  margin-bottom: 70rpx;
}

.type_list .saraly_ul{
  padding-bottom: 180rpx;
}

.type_list .base_ul .base_item{
  min-width:148rpx;
  height:58rpx;
  border-radius:29rpx;
  border:1rpx solid rgba(205,203,207,1);
  font-size:26rpx;
  color:rgba(93,86,93,1);
  text-align: center;
  margin-right: 26rpx;
  margin-bottom: 30rpx;
  float: left;
  line-height: 58rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.type_list .base_ul .base_item_active{
  border-color: #652791;
  background: #652791;
  color: white;
}

.button-box {
  background:rgba(255,255,255,1);
  display: flex;
  padding: 46rpx 40rpx;
  text-align: center;
  line-height: 88rpx;
  font-size:30rpx;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}

.button-box .btn_01{
  width:248rpx;
  height:88rpx;
  background:rgba(245,244,247,1);
  border-radius:44rpx 0rpx 0rpx 44rpx;
  color:rgba(93,86,93,1);
}

.button-box .btn_02{
  flex-grow: 1;
  height:88rpx;
  background:rgba(101,39,145,1);
  border-radius:0rpx 44rpx 44rpx 0rpx;
  color: white;
}

.type_list{
  padding: 0 40rpx;
  overflow-y: scroll;
}

.position_list{
  overflow-y: scroll;
  max-height: 892rpx;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}