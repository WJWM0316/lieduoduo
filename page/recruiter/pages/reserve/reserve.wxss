.place {
  height: 88rpx;
}
.office-status-bar{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #FFFFFF;
  box-shadow:0rpx 11rpx 30rpx 0rpx rgba(153,193,214,0.08);
  position: fixed;
  left: 0;
  z-index: 1;
  box-sizing: border-box;
  padding: 0 60rpx;
	font-size:28rpx;
	color:rgba(40,40,40,1);
}

.office-status-bar .item {
  padding-right: 32rpx;
  text-align: center;
  border-radius: 27rpx;
  position: relative;
  display: inline-block;
  margin-right: 80rpx;
}

.office-status-bar .tips {
  width:40rpx;
	height:40rpx;
	background:rgba(232,233,235,1);
	position: absolute;
	right: 60rpx;
	top: 50%;
	transform: translateY(-50%);
	border-radius: 50%;
	text-align: center;
	line-height: 40rpx;
	color: white;
	font-weight: 700;
}

.office-status-bar .text {
  display: inline-block;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  max-width: 120rpx;
  line-height: 88rpx;
  float: left;
}

.office-status-bar .text.active {
  color: #652791;
  font-weight: 700;
}

.office-status-bar .icon-zhishibiao {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
}

.item2 {
  background:rgba(255,255,255,1);
  /*box-shadow:0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);*/
  box-shadow:0rpx 0rpx 20rpx 0rpx rgba(50,14,59,0.08);
  border-radius:16rpx;
  position: relative;
  padding: 0 40rpx;
}

.item2 .row-one {
  display: flex;
  padding: 30rpx 0 40rpx 0;
  align-items: center;
  justify-content: center;
  align-items: center;
}

.item2 .img-box {
  width:120rpx;
  height:120rpx;
  border-radius: 50%;
  position: relative;
  box-shadow:0rpx 2rpx 12rpx 0rpx rgba(0,0,0,0.12);
  border:2rpx solid rgba(232,233,235,1);
}

.item2 .img-box .avatar{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.item2 .img-box .sexIcon {
	width: 32rpx;
	height: 32rpx;
	position: absolute;
	bottom: 0;
	right: 0;
}

.item2 .content {
  margin-left: 32rpx;
  max-height: 120rpx;
  flex: 1;
}

.item2 .header {
  line-height:1.2;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 450rpx;
  font-size: 26rpx;
  color: #929292;
  font-weight: 300
}

.item2 .header .name{
  font-size:36rpx;
  font-weight:700;
  color:rgba(40,40,40,1);
