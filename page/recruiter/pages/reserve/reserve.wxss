.place {
  height: 88rpx;
}
.office-status-bar{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #FFFFFF;
  box-shadow:0rpx 11rpx 30rpx 0rpx rgba(153,193,214,0.08);
  position: fixed;
  left: 0;
  z-index: 1;
  box-sizing: border-box;
  padding: 0 60rpx;
	font-size:28rpx;
	color:rgba(40,40,40,1);
}

.office-status-bar .item {
  padding-right: 32rpx;
  text-align: center;
  border-radius: 27rpx;
  position: relative;
  display: inline-block;
  margin-right: 88rpx;
}

.office-status-bar .tips {
  width:40rpx;
	height:40rpx;
	background:rgba(232,233,235,1);
	position: absolute;
	right: 60rpx;
	top: 50%;
	transform: translateY(-50%);
	border-radius: 50%;
	text-align: center;
	line-height: 40rpx;
	color: white;
	font-weight: 700;
}

.office-status-bar .text {
  display: inline-block;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  max-width: 120rpx;
  line-height: 88rpx;
  float: left;
}

.office-status-bar .icon-zhishibiao {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
}

.office-status-bar .active {
  font-weight:700;
  color:#282828;
  font-size:32rpx;
  pointer-events: none;
}


