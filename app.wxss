/**app.wxss**/
@import 'iconfont.wxss'
.container {
} 
.holderStyle{
  color: #BCBCBC;
	font-weight: 300;
}
.clear-style {
	background: none;
	border: 0;
	padding: 0;
}
.clear-style::after{
	display: none;
	border: none;
}
page {
	background:#FBFBFF;
}
text {
	word-break:break-all;
}
view {
	-webkit-overflow-scrolling: touch;
}
button::after{
	border: none;
}
.filter {
	width: 100%;
	height: 100%;
	-webkit-filter: blur(10rpx);
  filter: blur(10rpx);
  border-radius: 50%;
  display: block;
}
